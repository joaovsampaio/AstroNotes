{
  "expo": {
    "name": "AstroNotes",
    "slug": "AstroNotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.joaovsampaio.astronotes"
    },
    "web": {
      "favicon": "./app/assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O Aplicativo Precisa De Permiss√£o Para Acessar As Imagens."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c9459e11-5eed-4180-9b7a-d81a7590f50b"
      }
    }
  }
}
